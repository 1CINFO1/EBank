        <ListView fx:id="jobListView" layoutX="14.0" layoutY="14.0" prefHeight="400.0" prefWidth="572.0" />
        <Button layoutX="14.0" layoutY="430.0" mnemonicParsing="false" onAction="#refreshJobList" text="Refresh" />
        <Button layoutX="120.0" layoutY="430.0" mnemonicParsing="false" onAction="#addJob" text="Add Job" />
        <Button layoutX="220.0" layoutY="430.0" mnemonicParsing="false" onAction="#updateJob" text="Update Job" />
        <Button layoutX="320.0" layoutY="430.0" mnemonicParsing="false" onAction="#deleteJob" text="Delete Job" />
        <Button layoutX="420.0" layoutY="430.0" mnemonicParsing="false" onAction="#viewCondidates" text="View Candidates" />
        <Button layoutX="520.0" layoutY="430.0" mnemonicParsing="false" onAction="#showJobStatistics" text="Show Statistics" />
    </children>
</AnchorPane>
