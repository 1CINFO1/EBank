<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="45.0" layoutY="14.0" prefHeight="172.0" prefWidth="315.0" spacing="10.0">
         <children>
            <Label text="Add Job" />
            <TextField fx:id="posteField" promptText="Poste" />
            <TextField fx:id="sujetField" promptText="Sujet" />
            <TextField fx:id="typeField" promptText="Type" />
            <TextField fx:id="emplacementField" promptText="Emplacement" />
            <DatePicker fx:id="dateExpirationPicker" promptText="Date Expiration" />
            <Button fx:id="saveButton" mnemonicParsing="false" text="Save" />
         </children>
      </VBox>
   </children>
</AnchorPane>
